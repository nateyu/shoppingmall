<br>
    <table border="0" id="listTable" cellspacing="0" cellpadding="0">
      <thead>
        <tr>
          <th style="text-align: left; padding-left: 8px; font-size: 14px; background-color: darkgray; color: white" colspan="6">
            商场列表
          </th>
        </tr>
        <tr>
          <th width="100" height="30">
            商场名称
          </th>
          <th width="100">
            版本号
          </th>
          <th width="100">
            添加时间
          </th>
          <th width="100">
            操作
          </th>
        </tr>
      </thead>
      <tbody>
        <% @malls.each do |i| %>
            <tr>
              <td>
                <%= i.name %>
              </td>
              <td>
                <%= i.version %>
              </td>
              <td>
                <%= i.created_at %>
              </td>
              <td>
                <a href="<%= edit_mall_path(i)%>">修改</a>
                </td>
            </tr>
        <% end %>
      </tbody>
    </table>
    <div style="width: 90%">
        <input type="button" value="新增商场" onclick="doSwitchAction2(&#39;malls/new&#39;)" style="margin-top: 10px; float: right"> 
    </div>
    <div id="dialog" title="商场管理">
    </div>